{
	"name": "fuelphp/dependency",
	"description": "FuelPHP Dependency handling",
	"keywords": ["dependency", "injection", "dependencies"],
	"homepage": "https://github.com/fuelphp/dependency",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4"
	},
	"require-dev": {
		"codeception/codeception": "~2.0.0",
        "phpspec/phpspec": "2.1.*@dev,>=2.1.0-RC2",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
	},
	"autoload": {
		"psr-4": {
			"Fuel\\Dependency\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fuel\\Dependency\\Stub\\": "stub/"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
